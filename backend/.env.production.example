# Backend Production Environment Variables
# Copy this file to backend/.env.production and update values

# Database
MONGODB_URI=mongodb+srv://your-production-mongodb-uri
DB_NAME=shotly

# Server Configuration
PORT=3009
NODE_ENV=production

# Production Base URLs
BASE_URL=https://shorly.uk
FRONTEND_URL=https://shorly.uk

# JWT Configuration
JWT_SECRET=your-production-super-secret-jwt-key

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-production-google-client-id
GOOGLE_CLIENT_SECRET=your-production-google-client-secret
GOOGLE_CALLBACK_URL=https://api.shorly.uk/api/auth/google/callback
